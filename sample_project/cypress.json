{
  "baseUrl":"https://iprice.my/",
    "reporter":"mochawesome",
    "reporterOptions": {
        "reportDir": "sample_project/cypress/results",
        "reportFilename": "report",
        "overwrite": true,
        "html": true,
        "json": true
      },
      "video":false,
      "responseTimeout": 12000,
      "defaultCommandTimeout": 12000,
      "viewportWidth": 1000,
      "viewportHeight": 800,
      "retries":3,
      "testFiles": "**/*.{feature,features}"
}

